{
  "name": "vdp-prostore",
  "version": "1.0.0",
  "description": "E-commerse app built with MERN stack and a course from Brad Traversy",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix ../frontend/admin-app",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -destroy",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandarrellponce/vdp-prostore.git"
  },
  "author": "Van Darrell Ponce",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vandarrellponce/vdp-prostore/issues"
  },
  "homepage": "https://github.com/vandarrellponce/vdp-prostore#readme",
  "dependencies": {
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.6",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "http-proxy-middleware": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.4.4",
    "slugify": "^1.4.6",
    "socket.io": "^2.3.0",
    "validator": "^13.1.17"
  }
}
